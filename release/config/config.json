{
    "log": {
        "disabled": true,
        "level": "trace",
        "output": "/usr/local/sing-box/sing-box.log",
        "timestamp": true
    },
  "dns": {
          "servers": [
            {
                "tag": "cf-tls4",
                "address": "1.1.1.1",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "direct4"
            },
            {
                "tag": "cf-tls6",
                "address": "2606:4700:4700::1111",
                "address_strategy": "prefer_ipv6",
                "strategy": "prefer_ipv6",
                "detour": "direct6"
            },
            {
                "tag": "google-udp",
                "address": "8.8.8.8",
                "address_strategy": "prefer_ipv4",
                "strategy": "prefer_ipv4",
                "detour": "direct4"
            },
            {
                "tag": "block",
                "address": "rcode://success"
            }
        ],
        "rules": [
        {
          "domain": ["zippstorm.com","googletagservices.com"],
          "ip_version": 6,
          "server": "cf-tls6"
        },
        {
          "domain": ["zippstorm.com","googletagservices.com"],
          "ip_version": 4,
          "server": "cf-tls4"
        },
        {
          "geosite": "category-ads-all",
          "server": "block",
          "disable_cache": true
        }
        ],
        "final": "cf-tls4",
        "strategy": "prefer_ipv4",
        "disable_cache": false,
        "disable_expire": false
  },
  "inbounds": [
      {
        "type": "mixed",
        "tag": "socks-in",
        "listen": "::",
        "sniff": true,
        "sniff_override_destination": true,
        "tcp_fast_open": true,
        "listen_port": 50275,
        "users": [
          {
            "username": "xxxxxx",
            "password": "xxxxxxxxxx"
          }
        ]
      }
  ],
  "outbounds": [
    {
        "type": "direct",
        "tag": "direct4",
        "bind_interface" : "eth0",
        "domain_strategy": "prefer_ipv4"
    },
    {
        "type": "direct",
        "tag": "direct6",
        "bind_interface" : "eth0",
        "domain_strategy": "prefer_ipv6"
    },
    {
        "type": "direct",
        "tag": "warp",
        "bind_interface" : "warp",
        "domain_strategy": "prefer_ipv4"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "rule_set": [
      {
        "tag": "geosite-netflix",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-netflix.srs",
        "download_detour": "direct4"
      },
      {
        "tag": "geosite-dazn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-dazn.srs",
        "download_detour": "direct4"
      },
      {
        "tag": "geosite-disney",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-disney.srs",
        "download_detour": "direct4"
      }
    ],
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "rule_set": ["geosite-netflix", "geosite-disney"],
        "outbound": "direct4"
      }
    ],
    "final": "direct4"
  }
}
